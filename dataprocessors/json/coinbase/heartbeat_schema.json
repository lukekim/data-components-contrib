{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "type": "heartbeat",
            "sequence": 90,
            "last_trade_id": 20,
            "product_id": "BTC-USD",
            "time": "2014-11-07T08:19:28.464459Z"
        }
    ],
    "required": [
        "type",
        "sequence",
        "last_trade_id",
        "product_id",
        "time"
    ],
    "properties": {
        "type": {
            "type": "string",
            "examples": [
                "heartbeat"
            ]
        },
        "sequence": {
            "type": "integer",
            "examples": [
                90
            ]
        },
        "last_trade_id": {
            "type": "integer",
            "examples": [
                20
            ]
        },
        "product_id": {
            "$id": "#/properties/product_id",
            "type": "string",
            "title": "The product_id schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "BTC-USD"
            ]
        },
        "time": {
            "$id": "#/properties/time",
            "type": "string",
            "title": "The time schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "2014-11-07T08:19:28.464459Z"
            ]
        }
    },
    "additionalProperties": false
}